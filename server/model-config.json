{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "db",
    "public": false
  },
  "AccessToken": {
    "dataSource": "mysql-db",
    "public": false
  },
  "ACL": {
    "dataSource": "mysql-db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mysql-db",
    "public": false
  },
  "Role": {
    "dataSource": "mysql-db",
    "public": false
  },
  "Email": {
    "dataSource": "smtpDs"
  },
  "user": {
    "dataSource": "mysql-db",
    "public": true,
    "options": {
      "emailVerificationRequired": true,
      "realmRequired": true,
      "realmDelimiter": ":"
    }
  },
  "enterprise": {
    "dataSource": "mysql-db",
    "public": true
  }
}
